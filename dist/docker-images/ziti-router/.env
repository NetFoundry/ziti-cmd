
# interface on which to forward ports to container
ZITI_INTERFACE=0.0.0.0

# REQUIRED: router enrollment token as a string, not a path
ZITI_ROUTER_JWT=

# REQUIRED: controller ctrl.endpoint address
ZITI_CTRL_ADVERTISED_ADDRESS=
# optional: controller ctrl.endpoint port (default: 6262)
ZITI_CTRL_ADVERTISED_PORT=

# optional: router mode (default: host)
ZITI_ROUTER_MODE=
# optional: router docker image (default: openziti/ziti-router:latest)
ZITI_ROUTER_IMAGE=
